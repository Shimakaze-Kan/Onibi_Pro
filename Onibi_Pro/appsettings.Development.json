{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.SpaProxy": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "JwtSettings": {
    "Issuer": "OnibiPro",
    "ExpirationTimeInMinutes": 60,
    "Secret": "secretsecretsecretsecretsecretsecretsecret",
    "Audience": "OnibiPro"
  },
  "ConnectionStrings": {
    "RedisConnection": "localhost:90",
    "SqlServerConnection_McDowel": "Server=localhost\\MSSQLSERVER02;Database=Onibi_Pro;TrustServerCertificate=True;Integrated Security=true;",
    "SqlServerConnection_Onibi_MasterDb": "Server=localhost\\MSSQLSERVER02;Database=Onibi_MasterDb;TrustServerCertificate=True;Integrated Security=true;"
  },
  "ReverseProxy": {
    "Routes": {
      "notifications-route-api": {
        "ClusterId": "communication-notifications-route-api",
        "Match": {
          "Path": "/notifications"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/notifications"
          }
        ],
        "Metadata": {
          "methods": [ "GET" ]
        }
      },
      "chats-route-api": {
        "ClusterId": "communication-message-route-api",
        "Match": {
          "Path": "/messages/{*any}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/messages"
          },
          { "PathPattern": "{*any}" }
        ],
        "Metadata": {
          "methods": [ "GET" ]
        }
      },
      "notifications-route": {
        "ClusterId": "communication-notification-signalR-cluster",
        "Match": {
          "Path": "/NotificationsHub/{*any}"
        },
        "Transforms": [
          {
            "RequestHeadersCopy": "true"
          },
          {
            "RequestHeaderOriginalHost": "true"
          },
          {
            "RequestHeader": "Upgrade",
            "Set": "WebSocket"
          },
          {
            "RequestHeader": "Connection",
            "Set": "Upgrade"
          },
          {
            "X-Forwarded": "Set",
            "For": "Append",
            "Proto": "Append",
            "Prefix": "Append",
            "HeaderPrefix": "X-Forwarded-"
          },
          { "PathPattern": "{*any}" }
        ],
        "SessionAffinity": {
          "Enabled": true,
          "AffinityKeyName": "Yarp.Session"
        }
      },
      "chats-route": {
        "ClusterId": "communication-chats-signalR-cluster",
        "Match": {
          "Path": "/ChatsHub/{*any}"
        },
        "Transforms": [
          {
            "RequestHeadersCopy": "true"
          },
          {
            "RequestHeaderOriginalHost": "true"
          },
          {
            "RequestHeader": "Upgrade",
            "Set": "WebSocket"
          },
          {
            "RequestHeader": "Connection",
            "Set": "Upgrade"
          },
          {
            "X-Forwarded": "Set",
            "For": "Append",
            "Proto": "Append",
            "Prefix": "Append",
            "HeaderPrefix": "X-Forwarded-"
          },
          { "PathPattern": "{*any}" }
        ],
        "SessionAffinity": {
          "Enabled": true,
          "AffinityKeyName": "Yarp.Session"
        }
      },
      "client-route": {
        "ClusterId": "spa-cluster",
        "Match": {
          "Path": "/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      }
    },
    "Clusters": {
      "communication-notification-signalR-cluster": {
        "Destinations": {
          "Message/destination1": {
            "Address": "https://localhost:7171/NotificationsHub"
          }
        }
      },
      "communication-chats-signalR-cluster": {
        "Destinations": {
          "Message/destination1": {
            "Address": "https://localhost:7171/ChatsHub"
          }
        }
      },
      "communication-notifications-route-api": {
        "Destinations": {
          "Message/destination1": {
            "Address": "https://localhost:7171/notifications/getAll"
          }
        }
      },
      "communication-message-route-api": {
        "Destinations": {
          "Message/destination1": {
            "Address": "https://localhost:7171/messages"
          }
        }
      },
      "spa-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:44406/"
          }
        }
      }
    }
  }
}